<div class="container-fluid">

	<!-- Banner -->
	<div class="event-main-banner">
		<div class="event-banner-content">
			<%= image_tag 'logo_evento_cinza.png' %>
			<h3>Lorem ipsum dolor sit amet, consectetur adipisicing elit.</h3>
			<%= link_to "Fazer inscrição", root_path, class: 'event-banner-button' %>
		</div>
	</div>

	<!-- Evento -->

	<div id="evento">
		<%= image_tag 'o_evento.png', class: 'evento-imagem' %>
		<div class="evento-content">
			<h2>O evento</h2>
			<p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Consequatur impedit ducimus, quia corporis quo enim
