<div class="container-fluid">

	<!-- Banner -->
	<div class="event-main-banner">
		<div class="event-banner-content">
			<%= image_tag 'logo_evento_cinza.png' %>
			<h3>Inteligencia de mercado e big data.</h3>
			<%= link_to "Fazer inscrição", "https://www.sympla.com.br/empreendata--talk-class__461103", class: 'btn-evento big' %>
		</div>
