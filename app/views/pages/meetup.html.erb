		<div class="botao-inscricao">
		<%= link_to "Fazer inscrição", root_path, class: 'event-banner-button' %>
		</div>
	</div>

	<!-- Local -->

	<div id="local">
		<h2 class="text-center">Local do evento</h2>
		<div class="row">
			<div class="col-md-5 col-12">
			
				<p>Le Wagon São Paulo Coding Bootcamp<br>
					R. Mourato Coelho, 1404 - 21<br>
					Vila Madalena, São Paulo - SP</p><br>

				<p>Contato para suporte<br>
					suporte@suporte.com.br</p>
				</div>
		
			<div class="col-md-7 col-12 local-imagem">
				<%= image_tag 'mapa.png' %>
			</div>
		</div>
	</div>
</div>

<!-- Parceiros -->

<div id="parceiros">
	<h2 class="text-center">Parceiros</h2>
	<div class="row">
		<div class="col-12 col-sm-6 col-md-2">
			<div class="img-parceiros">
				<div class="img-content"></div>
			</div>
		</div>
		<div class="col-12 col-sm-6 col-md-2">
			<div class="img-parceiros">
				<div class="img-content"></div>
			</div>
		</div>
		<div class="col-12 col-sm-6 col-md-2">
			<div class="img-parceiros">
